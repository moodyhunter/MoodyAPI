        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@babel/core": "^7.18.5",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.4",
        "@mui/system": "^5.8.4",
        "@mui/x-data-grid": "^5.12.1",
        "@types/bcrypt": "^5.0.0",
        "bcrypt": "^5.0.1",
        "dayjs": "^1.11.3",
        "jotai": "^1.7.2",
        "long": "^5.2.0",
        "minimist": "^1.2.6",
        "next": "^12.1.6",
        "next-auth": "^4.5.0",
        "nextjs-progressbar": "^0.0.14",
        "nice-grpc": "^1.2.0",
        "protobufjs": "^6.11.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.43",
