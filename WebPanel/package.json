        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@babel/core": "^7.18.9",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.9.1",
        "@mui/system": "^5.9.1",
        "@mui/x-data-grid": "^5.14.0",
        "@types/bcrypt": "^5.0.0",
        "bcrypt": "^5.0.1",
        "dayjs": "^1.11.4",
        "jotai": "^1.7.6",
        "long": "^5.2.0",
        "minimist": "^1.2.6",
        "next": "^12.2.3",
        "next-auth": "^4.10.2",
        "nextjs-progressbar": "^0.0.14",
        "nice-grpc": "^1.2.0",
        "protobufjs": "^7.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^18.6.1",
